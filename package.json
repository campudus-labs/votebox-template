{
  "name": "votebox",
  "version": "1.0.0",
  "description": "To vote for stuff",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "clean": "rimraf out/",
    "lint": "eslint --ignore-pattern '*.spec.js' --ext .js --ext .jsx src/",
    "test": "mocha 'src/**/*.spec.js' --compilers js:babel-core/register --require ignore-styles",
    "watch": "parallelshell \"npm run watch:build\" \"npm run watch:test\"",
    "watch:build": "webpack-dev-server --hot --inline --display-error-details --config webpack.config.js",
    "watch:test": "npm test -- -w"
  },
  "keywords": [
    "vote",
    "votebox",
    "redux"
  ],
  "author": "Campudus",
  "repository": "https://github.com/campudus/votebox",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "cssnano": "^3.5.2",
    "eslint": "^2.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.1.0",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.5.0",
    "ignore-styles": "^1.2.0",
    "immutable": "^3.7.6",
    "mocha": "^2.4.5",
    "parallelshell": "^2.0.0",
    "postcss-color-function": "^2.0.0",
    "postcss-easy-import": "^1.0.1",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.1",
    "postcss-mixins": "^4.0.1",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^1.2.0",
    "postcss-url": "^5.1.1",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.4",
    "redux-logger": "^2.6.0",
    "redux-thunk": "^1.0.3",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
