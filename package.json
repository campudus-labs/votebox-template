{
  "name": "votebox",
  "version": "1.0.0",
  "description": "To vote for stuff",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "check": "npm run check:backend && npm run check:frontend && npm run lint",
    "check:frontend": "karma start karma-all.config.js --single-run",
    "check:backend": "babel-node \"./node_modules/isparta/bin/isparta\" cover --no-default-excludes --includes \"**/test/**\" --excludes \"**/node_modules/**\" \"./node_modules/mocha/bin/_mocha\" -- \"src/backend/**/*.spec.js\"",
    "clean": "rimraf out/ coverage/",
    "lint": "eslint --ext .js --ext .jsx src/",
    "test": "npm run test:backend -s && npm run test:frontend -s",
    "test:frontend": "karma start karma-quick.config.js --single-run",
    "test:backend": "babel-node ./node_modules/mocha/bin/_mocha \"src/backend/**/*.spec.js\" --colors",
    "watch": "parallelshell \"npm run watch:build -s\" \"npm run watch:test -s\"",
    "watch:build": "webpack-dev-server --history-api-fallback --hot --inline --display-error-details --config webpack.config.js",
    "watch:check": "watch \"npm run check -s\" src/",
    "watch:test": "parallelshell \"npm run watch:test:backend -s\" \"npm run watch:test:frontend -s\"",
    "watch:test:frontend": "karma start karma-quick.config.js --no-single-run",
    "watch:test:backend": "watch \"npm run test:backend -s\" src/backend/"
  },
  "keywords": [
    "vote",
    "votebox",
    "redux"
  ],
  "author": "Campudus",
  "repository": "https://github.com/campudus/votebox",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "cssnano": "^3.5.2",
    "eslint": "^2.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.1.0",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.5.0",
    "immutable": "^3.7.6",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "istanbul": "^0.4.2",
    "jsdom": "^8.1.0",
    "karma": "^0.13.21",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.5",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "parallelshell": "^2.0.0",
    "postcss-color-function": "^2.0.0",
    "postcss-easy-import": "^1.0.1",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.1",
    "postcss-mixins": "^4.0.1",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^1.2.0",
    "postcss-url": "^5.1.1",
    "raw-loader": "^0.5.1",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.4",
    "redux-logger": "^2.6.0",
    "redux-thunk": "^1.0.3",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
    "superagent": "^1.8.0-beta.2",
    "url-loader": "^0.5.7",
    "watch": "^0.17.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
